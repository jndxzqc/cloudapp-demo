{
  "index_patterns":[
    "automated-test-*"
  ],
  "order":0,
  "version":60001,
  "settings":{
    "index":{
      "number_of_shards":"1",
      "refresh_interval":"5s"
    }
  },
  "mappings":{
    "dynamic_templates":[
      {
        "message_field":{
          "path_match":"message",
          "mapping":{
            "norms":false,
            "type":"text"
          },
          "match_mapping_type":"string"
        }
      },
      {
        "string_fields":{
          "mapping":{
            "norms":false,
            "type":"text",
            "fields":{
              "keyword":{
                "ignore_above":64,
                "type":"keyword"
              }
            }
          },
          "match_mapping_type":"string",
          "match":"*"
        }
      }
    ],
    "properties":{
      "@timestamp":{
        "type":"date"
      },
      "@version":{
        "type":"keyword"
      },
      "agent":{
        "properties":{
          "ephemeral_id":{
            "type":"text"
          },
          "hostname":{
            "type":"text"
          },
          "id":{
            "type":"text"
          },
          "name":{
            "type":"text"
          },
          "type":{
            "type":"text"
          },
          "version":{
            "type":"text"
          }
        }
      },
      "asctime":{
        "type":"text"
      },
      "date":{
        "type":"date"
      },
      "threadName":{
        "type":"text"
      },
      "thread":{
        "type":"text"
      },
      "name":{
        "type":"keyword"
      },
      "levelname":{
        "type":"keyword"
      },
      "lineno":{
        "type":"integer"
      },
      "module":{
        "type":"keyword"
      },
      "funcName":{
        "type":"keyword"
      },
      "message":{
        "type":"text",
        "norms":false
      },
      "test_task_id":{
        "type":"keyword"
      },
      "case_suite_id":{
        "type":"keyword"
      },
      "case_suite_name":{
        "type":"keyword"
      },
      "testcase_id":{
        "type":"keyword"
      },
      "testcase_name":{
        "type":"keyword"
      },
      "type":{
        "type":"keyword"
      },
      "exception":{
        "properties":{
          "exception_class":{
            "type":"text"
          },
          "exception_message":{
            "type":"text"
          },
          "stacktrace":{
            "type":"text"
          }
        }
      },
      "input":{
        "properties":{
          "type":{
            "type":"text"
          }
        }
      },
      "log":{
        "properties":{
          "file":{
            "properties":{
              "path":{
                "type":"text"
              }
            }
          },
          "offset":{
            "type":"long"
          }
        }
      }
    }
  },
  "aliases":{

  }
}
